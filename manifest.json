{
  "name": "Health Alarm",
  "description": "Get up and exercise, do not sit too long.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["alarms"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon" : "icon.png",
    "default_title": "Health Alarm",
    "default_popup": "popup.html"
  }
}
